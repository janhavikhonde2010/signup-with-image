{
    "version": 2,
    "functions": {
      "api/**/*.js": {
        "maxDuration": 60
      }
    },
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  